@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "錯誤";
}

<h1 class="text-danger">錯誤。</h1>
<h2 class="text-danger">處理您的要求時發生錯誤。</h2><br />

<h3>@Model.Exception.Message</h3>

<p>Stack Trace:</p>
@Model.Exception.StackTrace.ToString()

<br /><br />@Html.ActionLink("回首頁",@Model.ActionName,@Model.ControllerName)<br /><br />

